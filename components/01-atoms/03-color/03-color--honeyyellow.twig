    <div>Hex: {{ honeyyellow.dark.hex }}</div>
    <div>RGB: {{ honeyyellow.dark.rgb }}</div>
    <div>CMYK: {{ honeyyellow.dark.cmyk }}</div>
  </div>
</div>


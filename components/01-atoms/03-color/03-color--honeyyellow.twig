  <div class="p-2 w-48 h-48 text-black text-xs font-mono" style="background-color: {{ honeyyellow.light.hex }}">
    <div>Name: {{ honeyyellow.light.label }}</div>
    <div>Class: {{ honeyyellow.light.class }}</div>

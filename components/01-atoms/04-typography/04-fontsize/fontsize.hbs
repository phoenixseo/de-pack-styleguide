<h1>Font Size</h1>

<table class="table-auto border-collapse w-full overflow-hidden">
<thead>
  <tr>
    <th>class</th>
    <th>rem</th>
    <th>px</th>
    <th>sample</th>
  </tr>
</thead>
<tbody>
  {{#each sizes}}
  <tr>
    <td>{{ class }}</td>
    <td>{{ rem }}</td>
    <td>{{ px }}</td>
    <td class="{{ class }}">The quick brown fox jumps over the lazy dog</td>
  </tr>
  {{/each}}
</tbody>
</table>
